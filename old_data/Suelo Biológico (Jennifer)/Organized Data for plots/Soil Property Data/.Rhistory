Groups <- c("Control A", "Treatment A", "Control B", "Treatment B")
Index <- c(8.57, 12.35, 12.09, 12.29)
sd <- c(1.17, 4.08, 3.96, 4.52)
data <- data.frame(Groups, Index, sd)
View(data)
ggplot(data, aes(x=Groups, y=Index)) +
geom_bar() +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=.2,
position=position_dodge(.9))
ggplot(data, aes(x=Groups, y=Index)) +
geom_bar(stat="identity", color="black",
position=position_dodge()) +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=.2,
position=position_dodge(.9))
theme_set(theme_bw(12))
ggplot(data, aes(x=Groups, y=Index)) +
geom_bar(stat="identity", color="black",
position=position_dodge()) +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=.2,
position=position_dodge(.9))
ggplot(data, aes(x=Groups, y=Index)) +
geom_bar(stat="identity", color="black", width = 2,
position=position_dodge()) +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=0.1,
position=position_dodge(.9))
ggplot(data, aes(x=Groups, y=Index)) +
geom_bar(stat="identity", color="black", width = 0.5,
position=position_dodge()) +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=0.1,
position=position_dodge(.9))
a <- ggplot(data, aes(x=Groups, y=Index)) +
geom_bar(stat="identity", color="black", width = 0.5,
position=position_dodge()) +
geom_errorbar(aes(ymin=Index, ymax=Index+sd), width=0.1,
position=position_dodge(.9))
a + ylab("Invsimpson")
a + ylab("Inverse Simpson Index")
